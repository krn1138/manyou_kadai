<p><ユーザー管理画面/p>

<table>
  <thead>
    <tr>
      <th>ユーザー名</th>
      <br>
      <th>詳しい内容</th>
      <th>終了期限</th>
      <th>ステータス</th>
      <th>登録日時</th>
      <th>優先度</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <div class="task_row"><% @users.each do |user| %></div>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <%= link_to 'Show', admin_user %>
        </td>
        <td><%= link_to 'Edit', edit_admin_user %></td>
        <td><%= link_to 'Destroy', admin_user, method: :delete, data: { confirm: 'Are you sure?' }</td>
      </tr>
    <% end %>
  </tbody>

  <br>
</table>



