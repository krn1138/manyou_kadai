<p><ユーザー管理画面/p>

<table>
  <thead>
    <tr>
      <th>ユーザー名</th>
    </tr>
  </thead>

  <tbody>
    <div class="user_row"><% @users.each do |user| %></div>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.created_at %></td>
        <td><%= user.updated_at %></td>
        <td><%= user.id %></td>
        <td><%= link_to 'Show', admin_user_path %></td>
        <td><%= link_to 'Edit', edit_admin_user_path %></td>
        <td><%= link_to 'Destroy', admin_user_path, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>

  <br>
</table>



