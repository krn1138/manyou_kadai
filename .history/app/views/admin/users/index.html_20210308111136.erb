<p>ユーザー管理画面</p>
<%= link_to "新規登録", new_admin_user_path %>

<table>
  <thead>
    <tr>
      <th>ユーザーid</th>
      <th>ユーザー名</th>
      <th>ユーザーemail</th>
      <th>作成日</th>
      <th>更新日</th>
    </tr>
  </thead>

  <tbody>
    <div class="user_row"><% @users.each do |user| %></div>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.created_at %></td>
        <td><%= user.updated_at %></td>
        <td><%= link_to 'Show', admin_user_path(user) %></td>
        <td><%= link_to 'Edit', edit_admin_user_path(user) %></td>
        <td><%= link_to 'Destroy', admin_user_path(user), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>

  <br>
</table>



